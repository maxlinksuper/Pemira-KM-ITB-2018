{
  "name": "pemira-km-itb-2018",
  "version": "1.0.0",
  "description": "Pemira KM ITB 2018 App",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . PemiraKMITB2018 --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager . PemiraKMITB2018 --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxlinksuper/Pemira-KM-ITB-2018.git"
  },
  "author": "Tanor Abraham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxlinksuper/Pemira-KM-ITB-2018/issues"
  },
  "homepage": "https://github.com/maxlinksuper/Pemira-KM-ITB-2018#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "electron": "^3.1.6",
    "jquery": "^3.3.1",
    "npm": "^6.7.0",
    "smalltalk": "^3.2.2"
  },
  "devDependencies": {
    "electron-packager": "^16.0.0"
  }
}
